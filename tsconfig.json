{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "lib": ["dom", "es2016", "esnext.asynciterable"],
      "moduleResolution": "node",
      "outDir": "src/dist",
      "baseUrl": "src",
      "strict": true,
      "strictPropertyInitialization": false,
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true
    },
    // "include": ["./index.ts"],
    "exclude": ["src/dist","node_modules"]
  }